<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- product.product.training.inherit inherit form view -->
        <record id="product_product_training_inherit" model="ir.ui.view">
            <field name="name">product.product.training.inherit.inherit.view.form</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <!-- Add your xpath here -->
                <field name="uom_po_id" position="after">
                    <field name="product_training" />
                </field>
            </field>
        </record>
    </data>
    
</odoo>